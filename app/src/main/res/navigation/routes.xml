<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/routes"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.hospitalproject.BlankFragment"
        android:label="doctors_list_fragment"
        tools:layout="@layout/doctors_list_fragment" >
        <action
            android:id="@+id/action_blankFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.hospitalproject.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>